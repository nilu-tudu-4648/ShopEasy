{
  "expo": {
    "name": "DriveLoop",
    "slug": "driveLoop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "driveLoop",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A6FFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.driveLoop.app"
    },
    "android": {
      "package": "com.driveLoop.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4A6FFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "57fbcfc8-9439-4f7f-a922-a81082da5964"
      }
    },
    "owner": "nilesh98"
  }
}
